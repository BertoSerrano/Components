{
   "NODE":{
      "name":"minibot",
      "path":"./node",
      "path_cls":["services","sensors"],
      "ethernet":"wlan0",
      "port_ns":9090,
      "port_node":4040,
      "start_port":5050,
      "def_frec":0.03,
      "def_worker":true
    },
    "services":{
 	       "gpioservice":{"cls":"gpioservice","gpio_mode":11},
         "i2cservice":{"cls":"i2cservice","-->":["gpioservice"]},
         "spiservice":{"cls":"spiservice","-->":["gpioservice"]}
    },
    "sensors":{
 	    "base":{"cls":"l298n","IN1":12, "IN2":13, "IN3":20, "IN4":21,"ENA":6,"ENB":26}
     }
}
