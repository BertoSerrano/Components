{
   "NODE":{
      "name":"simplebot1",
      "path":"./node",
      "etc":"<path>/etc",
      "path_cls":["services","sensors"],
      "ethernet":"enp0s25",
      "port_ns":9090,
      "port_node":5040,
      "start_port":6000,
      "def_frec":0.02,
      "def_worker":true
    },
   "services":{
        "usbserial":{"cls":"usbsimulator","comPort":"/dev/ttyS0","comPortBaud":115200,"mode":"local"}
   },
   "sensors":{
        "milaser":{"cls":"laser","LASER":[0,0]},
        "suelo":{"cls":"infrared","IR":[0,0,0,0]},
        "base":{"cls":"basemotion","MOTORS":[0,0],"BASE":[]},
        "pantilt":{"cls":"pantilt","PT":[90,90],"-->":["milaser"]}
    }
}
