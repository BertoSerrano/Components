{
   "NODE":{
      "name":"alphabot2",
      "path":"/home/pi/PYRO4DIR/pyro4bot/developing/node",
      "etc":"<path>/etc",
      "path_cls":["services","sensors"],
      "ethernet":"eth0",
      "name_server":true,
      "port_ns":9090,
      "port_node":4040,
      "start_port":5050,
      "def_frec":0.03,
      "def_worker":true
    },
   "services":{
        "picam":{"cls":"picam","path":"<path>","ethernet":"<ethernet>","framerate":25,"width":640,"height":480,"frec":0.02,"worker_run":true,"enable":true},
	      "gpioservice":{"cls":"gpioservice","gpio_mode":11,"frec":0.02,"enable":true,"worker_run":false}
   },
   "sensors":{
	    "l298n":{"cls":"l298n","IN1":12, "IN2":13, "IN3":20, "IN4":21,"ENA":6,"ENB":26,"-->":["gpioservice"],"frec":0.03, "enable":true,"worker_run":true}
    }
}
