#configuracion del nodo
{
   "NODE":{
      "name":"learnbot1",
      #"path":"/home/paco/Dropbox/td/developing/nodeRBB",
      "path":"/home/pi/td/developing/nodeRBB",
      "etc":"<path>/etc",
      "path_cls":["SERVICES","ROBOT"],
      #"ethernet":"wlp3s0",
      #"ethernet":"enp0s25",
      "ethernet":"eth0",
      "name_server":true,
      "port_ns":9090,
      "port_node":4040,
      "start_port":5050,
      "def_frec":0.03,
      "def_worker":true
    },
   "SERVICES":{
        "camera":{"cls":"picam","path":"<path>","framerate":25,"width":640,"height":480,"frec":0.02,"worker_run":true,"enable":true},
        "arduino":{"cls":"usbserial","comPort":"/dev/ttyS0","comPortBaud":115200,"frec":0.02,"enable":true}
   },
   "ROBOT":{
        "laser":{"cls":"laser","LASER":[0,0,0],"-->":["arduino"],"enable":true,"frec":0.02,"worker_run":true},
        "infrared":{"cls":"infrared","IR":[0,0,0,0],"-->":["arduino"],"enable":true,"frec":0.02,"worker_run":true},
        "base":{"cls":"basemotion","BASE":[0,0],"-->":["arduino"],"frec":0.03, "enable":true,"worker_run":true},
        "pantilt":{"cls":"pantilt","PT":[90,90],"-->":["arduino"],"frec":0.03, "enable":true,"worker_run":true}
    }
}
